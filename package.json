{
  "name": "castleofghosts",
  "version": "0.0.0",
  "description": "Браузерная игра",
  "repository": "https://github.com/PraktikumSeoulDevTeam/castleofghosts.git",
  "author": "Artyom Mozgovoy, Dmitry Kuznetsov, Nikita Ivanov",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12.13",
    "yarn": ">=1.22"
  },
  "scripts": {
    "start": "yarn server:run",
    "build": "run-p app:build server:build",
    "app:serve": "webpack serve -c ./app/webpack.config.js --mode development --watch",
    "app:build": "webpack -c ./app/webpack.config.js --mode production",
    "app:analyze": "webpack -c ./app/webpack.config.js --mode production --analyze",
    "app:levels": "node scripts/concatLevels.js ./app/levels ./tmp",
    "lint:verify": "eslint --max-warnings=0 \"**/*\"",
    "lint:fix": "eslint --fix \"**/*\"",
    "slint:verify": "stylelint --max-warnings=0 \"app/src/**/*.scss\"",
    "slint:fix": "stylelint --fix \"app/src/**/*.scss\"",
    "server:build": "run-s server:clear server:ts:compile",
    "server:serve": "run-p server:ts:watch server:nodemon",
    "server:ts:compile": "ttsc -p server",
    "server:ts:watch": "ttsc -p server -w",
    "server:nodemon": "nodemon dist/server/index.js",
    "server:run": "node dist/server/index.js",
    "server:clear": "rimraf ./dist/server",
    "docker:build": "docker build --tag seoulteam/castleofghosts:latest .",
    "docker:run": "docker run -p 3000:3000 --name castleofghosts seoulteam/castleofghosts",
    "docker:heroku": "run-s docker:heroku:push docker:heroku:release",
    "docker:heroku:push": "heroku container:push web",
    "docker:heroku:release": "heroku container:release web",
    "yarn:check": "yarn check --integrity",
    "yarn:fix": "yarn --update-checksums",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint:verify slint:verify yarn:check"
    }
  },
  "nodemonConfig": {
    "watch": [
      "dist/server/**/*.js"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "axios": "0.21.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "express-validator": "6.9.2",
    "formik": "2.2.5",
    "fscreen": "1.1.0",
    "lodash": "4.17.20",
    "mongodb": "3.6.4",
    "pg": "8.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.5.0",
    "sequelize-typescript": "2.1.0",
    "tslib": "2.1.0",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@babel/register": "7.12.10",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.9",
    "@types/fscreen": "1.0.1",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.165",
    "@types/mongodb": "3.6.8",
    "@types/node": "14.14.31",
    "@types/pg": "7.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.12",
    "@types/react-router-dom": "5.1.6",
    "@types/validator": "13.1.3",
    "@types/yup": "0.29.10",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.1.0",
    "babel-plugin-const-enum": "1.0.1",
    "copy-webpack-plugin": "7.0.0",
    "css-loader": "5.0.0",
    "eslint": "7.15.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "hook-shell-script-webpack-plugin": "0.1.3",
    "html-webpack-partials-plugin": "0.8.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "mini-css-extract-plugin": "1.2.1",
    "nodemon": "2.0.6",
    "npm-run-all": "4.1.5",
    "null-loader": "^4.0.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sass": "1.28.0",
    "sass-loader": "10.0.5",
    "style-loader": "2.0.0",
    "stylelint": "13.8.0",
    "stylelint-config-idiomatic-order": "8.1.0",
    "ts-loader": "8.0.14",
    "ttypescript": "1.5.12",
    "typescript": "4.0.5",
    "typescript-transform-paths": "2.2.3",
    "webpack": "5.4.0",
    "webpack-bundle-analyzer": "4.2.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-node-externals": "2.5.2"
  }
}
